<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".MainFragment" android:id="@+id/frameLayout">


    <Button
            android:text="FAQ's"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/faq"
            tools:layout_editor_absoluteX="162dp" app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintBottom_toTopOf="@+id/message"/>
    <Button
            android:text="Messages"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/message"
            tools:layout_editor_absoluteX="158dp" app:layout_constraintTop_toBottomOf="@+id/faq"
            app:layout_constraintBottom_toTopOf="@+id/favoutire"/>
    <Button
            android:text="My Favourites"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/favoutire"
            tools:layout_editor_absoluteX="140dp" app:layout_constraintTop_toBottomOf="@+id/message"
            app:layout_constraintBottom_toTopOf="@+id/account"/>
    <Button
            android:text="MyAccount"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/account"
            tools:layout_editor_absoluteX="150dp" app:layout_constraintTop_toBottomOf="@+id/favoutire"
            app:layout_constraintBottom_toTopOf="@+id/settting"/>
    <Button
            android:text="Settings"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/settting"
            tools:layout_editor_absoluteX="161dp" app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/account"/>
</androidx.constraintlayout.widget.ConstraintLayout>